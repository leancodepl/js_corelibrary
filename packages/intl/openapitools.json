{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.10.0",
    "generators": {
      "typescript-axios": {
        "generatorName": "typescript-axios",
        "output": "./src/api/api.generated",
        "glob": "./openapi-schema/*.{yaml,yml,json}",
        "additionalProperties": {
          "npmName": "@leancodepl/intl-poeditor-client",
          "supportsES6": true,
          "withNodeImports": true,
          "modelPropertyNaming": "camelCase"
        }
      }
    }
  }
} 