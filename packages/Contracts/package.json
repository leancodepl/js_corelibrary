{
  "name": "@leancode/contracts",
  "version": "6.1.8",
  "author": "LeanCode",
  "license": "ISC",
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "bin": {
    "ts-generator": "lib/index.js"
  },
  "keywords": [
    "contracts",
    "leancode"
  ],
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rollup --config rollup.config.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "proto": "npm-run-all -s proto:generate proto:definitions",
    "proto:generate": "pbjs -t static-module -w commonjs -o src/protocol/index.js src/protocol/contracts.proto --no-create --no-encode --lint \"eslint-disable prettier/prettier,no-undef,no-prototype-builtins,no-redeclare\"",
    "proto:definitions": "pbts -o src/protocol/index.d.ts src/protocol/index.js",
    "fmt:eslint": "eslint --fix src/**/*.{js,jsx,ts,tsx}"
  },
  "dependencies": {
    "@types/node": "^16.0.0",
    "cosmiconfig": "^7.0.0",
    "jest-validate": "^26.0.0",
    "lodash": "^4.0.0",
    "protobufjs": "^6.10.0",
    "yargs": "^17.0.0"
  },
  "peerDependencies": {
    "eslint": "^8.9.0",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@leancode/eslint-config": "^6.1.1",
    "@leancode/tools-rollup": "^6.1.1",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@types/concat-stream": "^1.6.0",
    "@types/download": "^8.0.0",
    "@types/lodash": "^4.0.0",
    "@types/yargs": "^17.0.0",
    "concat-stream": "^2.0.0",
    "download": "^8.0.0",
    "eslint": "^8.9.0",
    "npm-run-all": "^4.1.0",
    "rollup-plugin-preserve-shebang": "^1.0.0",
    "typescript": "^4.5.5"
  }
}
