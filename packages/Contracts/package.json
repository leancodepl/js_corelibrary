{
  "name": "@leancode/contracts",
  "version": "6.0.0",
  "author": "LeanCode",
  "license": "ISC",
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "bin": {
    "ts-generator": "lib/index.js"
  },
  "keywords": [
    "contracts",
    "leancode"
  ],
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rollup --config rollup.config.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "proto": "npm-run-all -s proto:generate proto:definitions",
    "proto:generate": "pbjs -t static-module -w commonjs -o src/protocol/index.js contracts.proto --no-create --no-encode",
    "proto:definitions": "pbts -o src/protocol/index.d.ts src/protocol/index.js"
  },
  "dependencies": {
    "cosmiconfig": "^7.0.0",
    "jest-validate": "^26.0.0",
    "lodash": "^4.0.0"
  },
  "peerDependencies": {
    "eslint": "^7.28.0",
    "typescript": "^4.3.0"
  },
  "devDependencies": {
    "@leancode/eslint-config": "^6.0.0",
    "@leancode/tools-rollup": "^6.0.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@types/concat-stream": "^1.6.0",
    "@types/lodash": "^4.0.0",
    "concat-stream": "^2.0.0",
    "eslint": "^7.28.0",
    "npm-run-all": "^4.1.0",
    "protobufjs": "^6.10.0",
    "typescript": "^4.3.0"
  }
}
