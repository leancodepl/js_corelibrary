{
  "name": "@leancodepl/image-uploader",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/image-uploader/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/image-uploader",
        "tsConfig": "packages/image-uploader/tsconfig.lib.json",
        "project": "packages/image-uploader/package.json",
        "entryFile": "packages/image-uploader/src/index.ts",
        "external": ["react", "react-dom", "react/jsx-runtime"],
        "rollupConfig": "@nx/react/plugins/bundle-rollup",
        "compiler": "swc",
        "assets": [
          {
            "glob": "packages/image-uploader/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/image-uploader/jest.config.ts"
      }
    },
    "sass-types": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx typed-scss-modules src --watch --nameFormat none",
        "cwd": "packages/image-uploader"
      }
    }
  }
}
